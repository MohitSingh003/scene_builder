<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scenebuilder.Controller">
   <children>
      <Button layoutX="284.0" layoutY="14.0" mnemonicParsing="false" onAction="#moveUP" text="UP" />
      <Button layoutX="14.0" layoutY="187.0" mnemonicParsing="false" onAction="#moveLeft" text="LEFT" />
      <Button layoutX="536.0" layoutY="187.0" mnemonicParsing="false" onAction="#moveRight" text="RIGHT" />
      <Button layoutX="273.0" layoutY="360.0" mnemonicParsing="false" onAction="#moveDown" text="DOWN" />
      <Circle fx:id="myCircle" fill="DODGERBLUE" layoutX="300.0" layoutY="212.0" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
   </children>
</AnchorPane>
